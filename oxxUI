function a:Tween(a, b, c, d)
    local e = t:Create(a, TweenInfo.new(c, d or Enum.EasingStyle.Quad, Enum.EasingDirection.Out), b)
    e:Play()
    return e
end
function a:Ripple(a)
    local b = a.AbsolutePosition
    local c = a.AbsoluteSize
    local d = Instance.new("Frame")
    d.Name = "Ripple"
    d.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    d.BackgroundTransparency = 0.75
    d.BorderSizePixel = 0
    d.Size = UDim2.new(0, 0, 0, 0)
    d.Position = UDim2.new(0, w.X - b.X - c.X / 2, 0, w.Y - b.Y - c.Y / 2)
    d.ZIndex = 10
    d.Parent = a
    local e = Instance.new("UICorner")
    e.CornerRadius = UDim.new(1, 0)
    e.Parent = d
    local f = math.max(c.X, c.Y)
    a:Tween(d, {
        Size = UDim2.new(0, f, 0, f),
        Position = UDim2.new(0, w.X - b.X - f / 2, 0, w.Y - b.Y - f / 2),
        BackgroundTransparency = 1
    }, 0.5)
    spawn(function()
        wait(0.5)
        d:Destroy()
    end)
end
function a:CreateWindow(a)
