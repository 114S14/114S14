            local h = false
            f.MouseButton1Click:Connect(function()
                f.Text = "Press a key..."
                h = true
            end)
            s.InputBegan:Connect(function(a)
                if h and a.UserInputType == Enum.UserInputType.Keyboard then
                    g = a.KeyCode
                    f.Text = b.Title .. ": [" .. g.Name .. "]"
                    h = false
                    if b.Callback then
                        b.Callback(g)
                    end
                end
            end)
            s.InputChanged:Connect(function(a)
                if g ~= Enum.KeyCode.Unknown and a.KeyCode == g and not h then
                    if b.Callback then
                        b.Callback(g)
                    end
                end
            end)
        end
