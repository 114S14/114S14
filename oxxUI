        m.PaddingLeft = UDim.new(0, 5)
        m.PaddingRight = UDim.new(0, 5)
        m.Parent = i
        if #b.Tabs == 0 then
            i.Visible = true
            a:Tween(d, {BackgroundColor3 = Oxx.Themes[a.Theme or "Dark"].Accent}, 0.2)
        end
        d.MouseButton1Click:Connect(function()
            for e, f in pairs(b.Tabs) do
                f.Frame.Visible = false
                a:Tween(f.Button, {BackgroundColor3 = Oxx.Themes[a.Theme or "Dark"].Second}, 0.2)
            end
            i.Visible = true
            a:Tween(d, {BackgroundColor3 = Oxx.Themes[a.Theme or "Dark"].Accent}, 0.2)
        end)
        d.MouseEnter:Connect(function()
            if not i.Visible then
                a:Tween(d, {BackgroundColor3 = Oxx.Themes[a.Theme or "Dark"].Accent}, 0.2)
            end
        end)
        d.MouseLeave:Connect(function()
            if not i.Visible then
                a:Tween(d, {BackgroundColor3 = Oxx.Themes[a.Theme or "Dark"].Second}, 0.2)
            end
        end)
        c.Button = d
        c.Frame = i
        table.insert(b.Tabs, c)
