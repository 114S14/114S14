            l.Font = Enum.Font.SourceSans
            l.TextXAlignment = Enum.TextXAlignment.Right
            l.Parent = c
            local m = b.Min or 0
            local n = b.Max or 100
            local o = b.Default or m
            local p = false
            j.Size = UDim2.new((o - m) / (n - m), 0, 1, 0)
            l.Text = tostring(o)
            g.InputBegan:Connect(function(a)
                if a.UserInputType == Enum.UserInputType.MouseButton1 then
                    p = true
                end
            end)
            s.InputChanged:Connect(function(a)
                if p and a.UserInputType == Enum.UserInputType.MouseMovement then
                    local b = math.clamp((a.Position.X - g.AbsolutePosition.X) / g.AbsoluteSize.X, 0, 1)
                    o = m + (n - m) * b
                    a:Tween(j, {Size = UDim2.new(b, 0, 1, 0)}, 0.1)
                    l.Text = tostring(math.floor(o))
                    if b.Callback then
                        b.Callback(o)
                    end
                end
            end)
            s.InputEnded:Connect(function(a)
                if a.UserInputType == Enum.UserInputType.MouseButton1 then
                    p = false
                end
            end)
        end
        function c:Keybind(b)
