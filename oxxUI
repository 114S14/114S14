                i.Parent = e
                local m = Instance.new("Frame")
                m.Name = "SatVal"
                m.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
                m.BorderSizePixel = 0
                m.Size = UDim2.new(1, -30, 1, -10)
                m.Position = UDim2.new(0, 5, 0, 5)
                m.Parent = e
                local n = i:Clone()
                n.Parent = m
                local o = Instance.new("Frame")
                o.Name = "Hue"
                o.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                o.BorderSizePixel = 0
                o.Size = UDim2.new(0, 20, 1, -10)
                o.Position = UDim2.new(1, -25, 0, 5)
                o.Parent = e
                local p = i:Clone()
                p.Parent = o
                local q = Instance.new("UIGradient")
                q.Color = ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 0)),
                    ColorSequenceKeypoint.new(0.17, Color3.fromRGB(255, 255, 0)),
                    ColorSequenceKeypoint.new(0.33, Color3.fromRGB(0, 255, 0)),
                    ColorSequenceKeypoint.new(0.5, Color3.fromRGB(0, 255, 255)),
                    ColorSequenceKeypoint.new(0.67, Color3.fromRGB(0, 0, 255)),
                    ColorSequenceKeypoint.new(0.83, Color3.fromRGB(255, 0, 255)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 0, 0))
                })
                q.Parent = o
                local r, s, t = l:ToHSV()
                local u = Instance.new("Frame")
                u.Name = "Cursor"
                u.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                u.BorderColor3 = Color3.fromRGB(0, 0, 0)
                u.Size = UDim2.new(0, 5, 0, 5)
                u.Position = UDim2.new(s, -2.5, 1 - t, -2.5)
                u.Parent = m
                local v = Instance.new("Frame")
                v.Name = "HueCursor"
                v.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                v.BorderColor3 = Color3.fromRGB(0, 0, 0)
                v.Size = UDim2.new(1, 0, 0, 2)
                v.Position = UDim2.new(0, 0, r, -1)
                v.Parent = o
                local w = false
                local x = false
                m.InputBegan:Connect(function(a)
                    if a.UserInputType == Enum.UserInputType.MouseButton1 then
                        w = true
                    end
                end)
                o.InputBegan:Connect(function(a)
                    if a.UserInputType == Enum.UserInputType.MouseButton1 then
                        x = true
                    end
                end)
                s.InputChanged:Connect(function(b)
                    if w then
                        s = math.clamp((b.Position.X - m.AbsolutePosition.X) / m.AbsoluteSize.X, 0, 1)
                        t = math.clamp(1 - (b.Position.Y - m.AbsolutePosition.Y) / m.AbsoluteSize.Y, 0, 1)
                        u.Position = UDim2.new(s, -2.5, 1 - t, -2.5)
                        l = Color3.fromHSV(r, s, t)
                        g.BackgroundColor3 = l
                        if b.Callback then
                            b.Callback(l)
                        end
                    end
                    if x then
                        r = math.clamp((b.Position.Y - o.AbsolutePosition.Y) / o.AbsoluteSize.Y, 0, 1)
                        v.Position = UDim2.new(0, 0, r, -1)
                        l = Color3.fromHSV(r, s, t)
                        g.BackgroundColor3 = l
                        m.BackgroundColor3 = Color3.fromHSV(r, 1, 1)
                        if b.Callback then
                            b.Callback(l)
                        end
                    end
                end)
                s.InputEnded:Connect(function(a)
                    if a.UserInputType == Enum.UserInputType.MouseButton1 then
                        w = false
                        x = false
                    end
                end)
                j.MouseButton1Click:Connect(function()
                    e:Destroy()
                    k = false
                end)
            end)
        end
        return c
    end
    return b
end
function a:Notify(a)
    local b = Instance.new("ScreenGui")
    b.Name = "Notification"
    b.ResetOnSpawn = false
    b.Parent = v.PlayerGui
    local c = Instance.new("Frame")
    c.Name = "Notification"
    c.BackgroundColor3 = Oxx.Themes[a.Theme or "Dark"].Main
    c.BorderSizePixel = 0
    c.Size = UDim2.new(0, 200, 0, 50)
    c.Position = UDim2.new(1, -210, 0, 10)
    c.Parent = b
    local d = Instance.new("UICorner")
    d.CornerRadius = UDim.new(0, 6)
    d.Parent = c
    local e = Instance.new("ImageLabel")
    e.Name = "Icon"
    e.BackgroundTransparency = 1
    e.Size = UDim2.new(0, 20, 0, 20)
    e.Position = UDim2.new(0, 10, 0.5, -10)
    e.Image = Oxx.Icons[a.Icon or "info"]
    e.Parent = c
    local f = Instance.new("TextLabel")
    f.Name = "Title"
    f.BackgroundTransparency = 1
    f.Size = UDim2.new(1, -40, 0, 20)
    f.Position = UDim2.new(0, 35, 0, 5)
    f.Text = a.Title or "Notification"
    f.TextColor3 = Oxx.Themes[a.Theme or "Dark"].Text
    f.TextSize = 14
    f.Font = Enum.Font.SourceSansBold
    f.TextXAlignment = Enum.TextXAlignment.Left
    f.Parent = c
    local g = Instance.new("TextLabel")
    g.Name = "Content"
    g.BackgroundTransparency = 1
    g.Size = UDim2.new(1, -40, 0, 20)
    g.Position = UDim2.new(0, 35, 0, 25)
    g.Text = a.Content or ""
    g.TextColor3 = Oxx.Themes[a.Theme or "Dark"].Placeholder
    g.TextSize = 12
    g.Font = Enum.Font.SourceSans
    g.TextXAlignment = Enum.TextXAlignment.Left
    g.Parent = c
    spawn(function()
        wait(a.Duration or 3)
        a:Tween(c, {Position = UDim2.new(1, 0, 0, 10)}, 0.5)
        wait(0.5)
        b:Destroy()
    end)
end
setmetatable(Oxx, {
    __call = function(b, c)
        return a:CreateWindow(c)
    end
})
return Oxx
