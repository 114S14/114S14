    n.Name = "Content"
    n.BackgroundTransparency = 1
    n.Size = UDim2.new(1, -150, 1, -30)
    n.Position = UDim2.new(0, 150, 0, 30)
    n.Parent = d
    local o = false
    local p
    local q
    f.InputBegan:Connect(function(a)
        if a.UserInputType == Enum.UserInputType.MouseButton1 then
            o = true
            p = a.Position
            q = d.Position
        end
    end)
    s.InputChanged:Connect(function(a)
        if o and a.UserInputType == Enum.UserInputType.MouseMovement then
            local b = a.Position - p
            d.Position = UDim2.new(q.X.Scale, q.X.Offset + b.X, q.Y.Scale, q.Y.Offset + b.Y)
        end
    end)
    s.InputEnded:Connect(function(a)
        if a.UserInputType == Enum.UserInputType.MouseButton1 then
            o = false
        end
    end)
    j.MouseButton1Click:Connect(function()
        c:Destroy()
    end)
    s.InputBegan:Connect(function(a)
        if a.KeyCode == b.ToggleKey then
            b.Visible = not b.Visible
            c.Enabled = b.Visible
        end
    end)
    function b:Tab(b)
